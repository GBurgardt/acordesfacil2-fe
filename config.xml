<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.acordesfacil" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AcordesFacil</name>
    <description>
        AcordesFacil
    </description>
    <author email="germanandresburgardt@gmail.com" href="">
        Gotoxy
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
        <icon density="ldpi" src="res/newIcon/android/mipmap-mdpi/icon.png" />
        <icon density="hdpi" src="res/newIcon/android/mipmap-hdpi/icon.png" />
        <icon density="mdpi" src="res/newIcon/android/mipmap-mdpi/icon.png" />
        <icon density="xhdpi" src="res/newIcon/android/mipmap-xhdpi/icon.png" />
        <icon density="xxhdpi" src="res/newIcon/android/mipmap-xxhdpi/icon.png" />
        <icon density="xxxhdpi" src="res/newIcon/android/mipmap-xxxhdpi/icon.png" />
    </platform>
    <plugin name="cordova-plugin-browsersync" spec="^1.1.0" />
    <engine name="android" spec="^7.1.1" />
</widget>
